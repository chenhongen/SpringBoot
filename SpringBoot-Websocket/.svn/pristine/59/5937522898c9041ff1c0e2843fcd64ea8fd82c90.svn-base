  <!--
   mac：关联station来确认产线及工位信息
   vinCode：关联ving得到dmsno来确认本轮推送工位
   -->
  <select id="selectStationByScan" resultMap="StationResultMap">
	with currentStation as (
	    select distinct ws.factory_code,ws.workshop_code,ws.workshop_line,v.dmsno
	    ,max(nvl(s.station_order,0)+1) over(partition by ws.workshop_line) as current_order
	    from V_WORKSHOP_STATION ws
	      left join V_SCAN s on ws.factory_code =s.factory_code and s.active_flag = 1
	      and ws.workshop_code = s.workshop_code and ws.workshop_line = s.workshop_line
	      left join V_VING v on v.vin_code = #{vinCode,jdbcType=VARCHAR}
	    where ws.station_mac = #{mac,jdbcType=VARCHAR} and ws.active_flag = 1
	  )
	  select ws.factory_code,ws.workshop_code,ws.workshop_line,ws.station_id,ws.station_mac
	    ,s.vin_code,ws.station_order,c.dmsno
	    from V_WORKSHOP_STATION ws
	    left join currentStation c on ws.factory_code = c.factory_code
	    and ws.workshop_code = c.workshop_code and ws.workshop_line = c.workshop_line
	    left join V_SCAN s on ws.station_id = s.station_id and c.dmsno = s.dmsno
	    where ws.active_flag = 1 and ws.station_order > 0
	    and current_order >= ws.station_order
	    order by ws.station_order asc
  </select>
